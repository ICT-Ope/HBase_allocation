+<%import>
+org.apache.hadoop.conf.Configuration;
+org.apache.hadoop.hbase.HBaseConfiguration;
+org.apache.hadoop.hbase.HTableDescriptor;
+org.apache.hadoop.hbase.MasterNotRunningException;
